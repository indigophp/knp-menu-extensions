{
    "name": "indigophp/knp-menu-extensions",
    "description": "Extensions for Knp Menu using League components",
    "license": "MIT",
    "keywords": ["league", "knp", "menu"],
    "homepage": "https://indigophp.com",
    "authors": [
        {
            "name": "Márk Sági-Kazár",
            "email": "mark.sagikazar@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "knplabs/knp-menu": "2.0.*@dev",
        "league/container": "1.3.*@dev"
    },
    "require-dev": {
        "phpspec/phpspec": "~2.1",
        "henrikbjorn/phpspec-code-coverage" : "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Indigo\\KnpMenu\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
